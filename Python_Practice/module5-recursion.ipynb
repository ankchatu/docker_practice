    "# iterative way factorial\n",
    "\n",
    "def iter_fact(n):\n",
    "    factorial = 1\n",
    "    for i in range(1,n+1):\n",
    "        factorial *= i\n",
    "    return factorial\n",
    "\n",
    "iter_fact(5)"
   ]
